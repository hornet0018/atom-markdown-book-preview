{
  "name": "MDBP-markdown-book-preview",
  "main": "./lib/markdown-book-preview",
  "version": "1.4.0",
  "description": "Atom Package markdown preview with VivlioStyle",
  "keywords": [
    "markdown",
    "preview",
    "VivlioStyle"
  ],
  "activationCommands": {
    "atom-workspace": [
      "markdown-book-preview:toggle",
      "markdown-book-preview:toggleVS",
      "markdown-book-preview:printPDF",
      "markdown-book-preview:exportXML",
      "markdown-book-preview:downloadTemplate"
    ]
  },
  "repository": "https://github.com/lwohtsu/atom-markdown-book-preview",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "chokidar": "^2.0.0",
    "highlight.js": "^9.12.0",
    "image-size": "^0.6.2",
    "jsdom": "^11.5.1",
    "lodash": "^4.17.4",
    "marked": "^0.3.9",
    "node-static": "^0.7.10",
    "path": "^0.12.7"
  }
}

[
  {
    "f": "@div:([a-z|0-9 ]+)",
    "r": "<div class=\"$1\">"
  },
  {
    "f": "@divend",
    "r": "<\/div>"
  },
  {
    "f": "<hr>",
    "r": "<hr class=\"pagebreak\">"
  },
  {
    "f": "@negative:(\\d+)",
    "r": "<div style=\"margin-top:-$1mm\"><\/div>"
  },
  {
    "f": "<p>(<div class=\"[^\"]+\">)</p>",
    "r": "$1"
  },
  {
      "f": "<li[^>]*>▶",
      "r": "<li class=\"aokome\"><span>▶</span>"
  },
  {
      "f": "<li[^>]*>([\u2776-\u277F\u24EB-\u24F4]+)",
      "r": "<li class=\"akakome\"><span>$1</span>"
  },
  {
      "f": "<span class=\"hljs-comment\"([^>]*)>#←",
      "r": "<span class=\"hljs-comment codered\"$1>"
  },
  {
    "f": "<p><\/div><\/p>",
    "r": "<\/div>"
  },
  {
    "f": "<p><\/div>",
    "r": "<\/div>"
  },
  {
    "f": "<\/div><\/p>",
    "r": "<\/div>"
  },
  {
    "f": "(<div class=\"[^\"]+\">)</p>",
    "r": "$1"
  },
  {
    "f": "<p>(<div class=\"[^\"]+\">)",
    "r": "$1"
  },
  {
    "f": "(<hr class=\"[^\"]+\">)</p>",
    "r": "$1"
  },
  {
    "f": "<p>(<hr class=\"[^\"]+\">)",
    "r": "$1"
  },
  {
    "f": "<p>(<svg [^>]+>)<\/p>",
    "r": "$1"
  },
  {
    "f": "<p>(<image [^>]+>)",
    "r": "$1"
  },
  {
    "f": "(<image [^>]+>)<\/p>",
    "r": "$1"
  },
  {
    "f": "<p>(<\/svg>)",
    "r": "$1"
  },
  {
    "f": "(<\/svg>)<\/p>",
    "r": "$1"
  },
  {
    "f": "<p><\/p>",
    "r": ""
  }
]
